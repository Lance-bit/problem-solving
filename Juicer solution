
#include <iostream>
using namespace std;

int main() {
    
int n , b , d;
cin>>n>>b>>d;
 long long int sum =0, counter=0;
long long arr[n];

    for(int i = 0 ;i<n;i++)
    cin>>arr[i];

for(int i = 0 ; i<n;i++){
    if(arr[i]<=b)
    sum+=arr[i];
    
    if(sum > d){
    counter++;
    sum=0;}
}
    cout<<counter;




    




}
